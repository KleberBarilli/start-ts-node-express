{
  "name": "ts-node-express-start",
  "version": "1.0.0",
  "description": "Esqueleto do Ã­nicio de uma api usando typescript, nodejs, express, prettier/eslint, jest etc...",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/shared/server.js",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "test": "jest"
  },
  "heroku-run-build-script": true,
